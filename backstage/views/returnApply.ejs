<script>
	$("#returnTable").on('click','.passApply',function (event) {
		var udid=$(event.target).parents('.center').data('udid');
        $.ajax({
            url: 'changeReturnApply',
            type: 'POST',
            dataType: 'text',
            data: {returnApplyStatus: '1',udid:udid}
        })
            .done(function(data) {
                if(data=='1'){
                    $(event.target).parents('tr').remove();
				}else{
                    alert('未知错误，请刷新后再处理！');
				}
            });

    });
    $("#returnTable").on('click','.failApply',function (event) {
        var udid=$(event.target).parents('.center').data('udid');
        $('#returnModal').modal('show');
        $('#failReasonSubmit').click(function (){
            console.log($('#inputReason').val());
            var failReason=$('#inputReason').val();
                $.ajax({
                    url: 'changeReturnApply',
                    type: 'POST',
                    dataType: 'text',
                    data: {returnApplyStatus: '0',udid:udid,failReason:failReason}
                })
                    .done(function(data) {
                        if(data=='1'){
                            $(event.target).parents('tr').remove();
                        }else{
                            alert('未知错误，请刷新后再处理！');
                        }
                    });
        });
    });
